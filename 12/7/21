import java.util.*;
class sunrises1
{
public static void main(String[]args)
throws IO Exception
{
Scanner sc=new Scanner(System.in);
char ch[]=str.tocharArray();
stringBuffer s = new stringBuilder();
for(int i=0;i<Str.lengh();i++)
{
if(ch==str.charAt(i));
s.append(Charecter.toUpperCase(Str.charAt(i)));
elseif(s.append(Str.charAt(i)));
ch=strcharAt(i);
String s1;
s1=s1.toLowerCase();
}
System.out.println("the coding class(s1)");
}
}

